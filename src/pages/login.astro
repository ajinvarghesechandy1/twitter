---
import '../css/auth.css'

---
<title>
    Login
</title>
<div class="bg">
    <div class="box">
        <h1>
            Login
        </h1>
      <form action="">
          <input type="text" name="username" placeholder="username" required>
          <input type="password" name="password" placeholder="********" required><br>
          <input class="auth-btn" type="submit" value="Login">
      </form>
    </div>
  </div>
  